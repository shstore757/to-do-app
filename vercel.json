{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api-to-do-blue.vercel.app//api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "BACKEND/wsgi.py"
    }
  ],
  "builds": [
    {
      "src": "BACKEND/",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ]
}
