{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://backend-to-do-chi.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "BACKEND/wsgi.py"
    }
  ],
  "builds": [
    {
      "src": "BACKEND/",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "taskapi/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ]
}
